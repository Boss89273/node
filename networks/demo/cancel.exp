#!/usr/bin/expect

set id [lindex $argv 0]
set from [lindex $argv 1]
set chain_id [lindex $argv 2]
set home [lindex $argv 3]

set timeout 30
	if {"$home" == ""} {
		spawn ./bnbcli dex cancel -i $id --from $from --chain-id=$chain_id -f $id
	} else {
		spawn ./bnbcli dex cancel --home $home -i $id --from $from --chain-id=$chain_id -f $id
	}
	expect "Password*"
	send "12345678\r"
interact
